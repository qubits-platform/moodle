<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/qbassign/submission/scratch/db" VERSION="20120423" COMMENT="XMLDB file for Moodle mod/qbassign/submission/scratch"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="qbassignsubmission_scratch" COMMENT="Info about scratch submission">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="qbassignment" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="submission" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="scratch" TYPE="binary" NOTNULL="false" SEQUENCE="false" COMMENT="The text for this Scratch submission."/>
        <FIELD NAME="onlineformat" TYPE="int" LENGTH="4" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The format for this Scratch submission."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for this scratch submission."/>
        <KEY NAME="qbassignment" TYPE="foreign" FIELDS="qbassignment" REFTABLE="qbassign" REFFIELDS="id" COMMENT="The qbassignment instance this Scratch submission relates to."/>
        <KEY NAME="submission" TYPE="foreign" FIELDS="submission" REFTABLE="qbassign_submission" REFFIELDS="id" COMMENT="The submission this Scratch submission relates to."/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
